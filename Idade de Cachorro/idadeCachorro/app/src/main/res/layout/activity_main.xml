<?xml version="1.0" encoding="utf-8"?><RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/colorAccent"    tools:context="lucasfscr.com.idadecachorro.MainActivity"    >    <TextView        android:id="@+id/resultado"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginStart="20dp"        android:layout_marginEnd="20dp"        android:layout_below="@+id/circularButton"        android:text=""        android:layout_marginTop="30dp"        android:textAlignment="center"        android:textColor="@color/white"        android:textSize="15sp"/>    <ImageView        android:id="@+id/imageView"        android:layout_width="100dp"        android:layout_height="100dp"        android:layout_alignParentTop="true"        android:layout_centerHorizontal="true"        android:layout_marginTop="30dp"        android:src="@drawable/cachorro"/>    <com.rengwuxian.materialedittext.MaterialEditText        android:id="@+id/materialEditText"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentStart="true"        android:layout_below="@+id/explain"        android:layout_marginStart="20dp"        android:layout_marginEnd="20dp"        android:layout_marginTop="12dp"        android:hint="@string/hint_field"        android:textColor="@color/white"        android:textSize="15sp"        android:textAlignment="center"        app:met_textColorHint="@color/gray"        app:met_baseColor="@color/white"        app:met_maxCharacters="2"        app:met_minCharacters="1"/>    <com.nihaskalam.progressbuttonlibrary.CircularProgressButton        xmlns:app="http://schemas.android.com/apk/res-auto"        android:id="@+id/circularButton"        android:layout_width="196dp"        android:layout_height="64dp"        android:layout_alignEnd="@+id/materialEditText"        android:layout_alignStart="@+id/materialEditText"        android:layout_below="@+id/materialEditText"        android:layout_marginTop="17dp"        android:textColor="@color/text_state_selector"        android:textSize="22sp"        app:pb_colorCancelText="@color/pb_orange"        app:pb_colorCompleteText="@color/pb_orange"        app:pb_colorErrorText="@color/pb_blue"        app:pb_colorIndicator="@color/colorButtonCompleteState"        app:pb_cornerRadius="30dp"        app:pb_iconCancel="@drawable/ic_action_cross"        app:pb_iconComplete="@drawable/ic_action_accept"        app:pb_iconError="@drawable/ic_action_cancel"        app:pb_selectorCancel="@color/pb_cancel_state_selector"        app:pb_selectorComplete="@color/complete_state_selector"        app:pb_selectorError="@color/error_state_selector"        app:pb_selectorIdle="@color/idle_state_selector"        app:pb_textColorAfterClick="@color/colorWhite"        app:pb_textIdle="@string/submit"/>    <TextView        android:id="@+id/explain"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/imageView"        android:layout_centerHorizontal="true"        android:textAlignment="center"        android:textColor="@color/white"        android:textAllCaps="true"        android:textSize="15sp"        android:layout_marginTop="20dp"        android:layout_marginEnd="20dp"        android:layout_marginStart="20dp"        android:text="@string/explain"/></RelativeLayout>